    "@types/jest": "^27.4.1",
    "jest": "^27.5.1",
    "@types/js-yaml": "^4.0.5",
    "ts-jest": "^27.1.3",
    "@types/lodash": "^4.14.181"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
