{
  "extends": "@rushstack/heft-node-rig/profiles/default/config/jest.config.json",
  "collectCoverage": true,
  "coveragePathIgnorePatterns": [
    "<rootDir>/src/aws/awsService.ts",
    "<rootDir>/src/aws/clients/*",
    "<rootDir>/src/types/relationshipDDBItem",
    "<rootDir>/src/services/secretsService.ts",
    "<rootDir>/src/interfaces/listUsersForRoleRequest.ts",
    "<rootDir>/src/index.ts"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 70,
      "functions": 70,
      "lines": 80,
      "statements": 75
    }
  },
  "coverageReporters": ["json-summary", "json"]
}
