{
  "name": "@amzn/workbench-core-infrastructure",
  "version": "0.1.0",
  "bin": {
    "infrastructure": "lib/index.js"
  },
  "scripts": {
    "build:local": "heft build --clean && lambda-build -e src/init.ts && mkdir -p ./lambda-build && mv archive.zip ./lambda-build",
    "build": "heft build --clean",
    "make-badges": "istanbul-badges-readme --coverageDir=./temp/coverage",
    "test": "heft test --clean && rushx make-badges",
    "test:update": "heft test -u --clean",
    "build:test": "rushx build && rushx test",
    "test:only": "heft test --clean",
    "watch": "tsc -w",
    "cdk": "cdk",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:synth": "cdk synth",
    "cdk:deploy": "cdk deploy --all --require-approval never --outputs-file cdk-outputs.json"
  },
  "devDependencies": {
    "@types/node": "^14",
    "aws-cdk": "^2.12.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "@rushstack/heft": "^0.45.0",
    "@rushstack/heft-jest-plugin": "^0.2.3",
    "@rushstack/heft-node-rig": "^1.7.1",
    "@types/heft-jest": "1.0.2",
    "istanbul-badges-readme": "1.8.1",
    "@amzn/eslint-config-workbench-core-eslint-custom": "workspace:*",
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.12.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.12.0-alpha.0",
    "eslint": "^8.7.0",
    "eslint-plugin-security": "^1.4.0",
    "@rushstack/eslint-config": "^2.5.1",
    "@types/jest": "^27.4.1",
    "@aws-cdk/assert": "^2.0.0",
    "jest": "^27.5.1",
    "express": "^4.17.3",
    "@types/express": "^4.17.13",
    "@vendia/serverless-express": "^4.5.4",
    "@types/aws-lambda": "^8.10.92",
    "axios": "^0.27.1",
    "cdk-nag": "^2.12.25"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.0.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.16",
    "@amzn/workbench-core-express": "workspace:*"
  }
}
