{
  "name": "@aws/workbench-core-swb-common-ui",
  "version": "0.1.2",
  "description": "Access and management of data files for the purpose of connecting to various AWS environments.",
  "homepage": "https://github.com/awslabs/solution-spark-on-aws",
  "repository": {
    "type": "git",
    "url": "https://github.com/awslabs/solution-spark-on-aws.git"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
    "url": "http://aws.amazon.com"
  },
  "main": "lib/index.jsx",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "heft build --clean && rushx pkg-json-lint",
    "build:test": "heft test --clean && rushx pkg-json-lint && rushx make-badges",
    "lint": "next lint --max-warnings 0 && rushx pkg-json-lint",
    "lint:fix": "eslint . --fix",
    "depcheck": "depcheck",
    "license-checker": "license-checker --onlyAllow 'MIT; Apache-2.0; ISC; BSD'",
    "make-badges": "istanbul-badges-readme --coverageDir=./temp/coverage --exitCode=1",
    "pkg-json-lint": "npmPkgJsonLint -c ../../.npmpackagejsonlintrc.json .",
    "sort-package-json": "sort-package-json package.json",
    "serve": "node .next/server/webpack-runtime.js",
    "test": "rushx test:only && rushx make-badges",
    "test:only": "heft test --clean --no-build"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": ".test.ts",
    "transform": {
      "\\.(ts)$": "ts-jest"
    }
  },
  "dependencies": {
    "date-fns": "^2.28.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "typescript": "^4.5.2",
    "axios": "^0.27.1",
    "express": "^4.17.3",
    "jwt-decode": "^3.1.2",
    "next": "^12.1.5",
    "next-global-css": "^1.2.0",
    "next-transpile-modules": "^9.0.0",
    "pkce-challenge": "^3.0.0",
    "swr": "^1.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@aws/eslint-config-workbench-core-eslint-custom": "workspace:*",
    "@cloudscape-design/collection-hooks": "^1.0.3",
    "@cloudscape-design/components": "^3.0.27",
    "@cloudscape-design/design-tokens": "^3.0.1",
    "@cloudscape-design/global-styles": "^1.0.1",
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@rushstack/eslint-config": "^2.5.1",
    "@rushstack/heft": "^0.45.0",
    "@rushstack/heft-jest-plugin": "^0.2.3",
    "@rushstack/heft-node-rig": "^1.7.1",
    "@types/heft-jest": "1.0.2",
    "@types/node": "^14",
    "@types/react": "17.0.38",
    "@types/uuid": "^8.3.4",
    "eslint-config-next": "12.1.5",
    "sass": "^1.49.8",
    "eslint": "^8.7.0",
    "eslint-plugin-import": "^2.26.0",
    "npm-package-json-lint": "^6.3.0",
    "npm-package-json-lint-config-default": "^5.0.0",
    "sort-package-json": "^1.57.0"
  }
}
