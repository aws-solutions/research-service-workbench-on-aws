ignores: [
    '@amzn/eslint-config-workbench-core-eslint-custom',
    '@rushstack/heft-jest-plugin',
    '@rushstack/heft-node-rig',
    '@types/heft-jest',
    '@types/jest',
    'eslint-plugin-import',
    'eslint-plugin-security',
    'npm-package-json-lint-config-default',
    '@types/node'
]
ignore-patterns: [
    // dirs matching these patterns will be ignored
    '.heft',
    '.rush',
    'config',
    'lib',
    'node_modules',
    'temp'
