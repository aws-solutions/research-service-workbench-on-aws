{
  "name": "@amzn/workbench-core-example-auth-backend",
  "version": "0.0.1",
  "description": "Example Auth Server",
  "homepage": "https://github.com/awslabs/monorepo-for-service-workbench",
  "repository": {
    "type": "git",
    "url": "https://github.com/awslabs/monorepo-for-service-workbench.git"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
    "url": "http://aws.amazon.com"
  },
  "main": "lib/server.js",
  "typings": "lib/server.d.ts",
  "scripts": {
    "build": "heft build --clean && rushx pkg-json-lint",
    "build:test": "heft test --clean && rushx pkg-json-lint",
    "depcheck": "depcheck",
    "lint:fix": "eslint . --fix",
    "make-badges": "echo 'NO TEST COVERAGE, SO NO BADGE !'",
    "pkg-json-lint": "npmPkgJsonLint -c ../../../.npmpackagejsonlintrc.json .",
    "sort-package-json": "sort-package-json package.json",
    "start": "node lib/server.js",
    "test": "rushx test:only",
    "test:only": "heft test --clean --no-build"
  },
  "dependencies": {
    "@amzn/workbench-core-authentication": "workspace:*",
    "@amzn/workbench-core-authorization": "workspace:*",
    "@amzn/workbench-core-logging": "workspace:*",
    "express": "^4.17.3"
  },
  "devDependencies": {
    "@amzn/eslint-config-workbench-core-eslint-custom": "workspace:*",
    "@rushstack/eslint-config": "^2.5.1",
    "@rushstack/heft": "^0.45.0",
    "@rushstack/heft-jest-plugin": "^0.2.3",
    "@rushstack/heft-node-rig": "^1.7.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/heft-jest": "1.0.2",
    "@types/node": "^14",
    "cookie-parser": "^1.4.6",
    "depcheck": "^1.4.3",
    "eslint": "^8.7.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-security": "^1.4.0",
    "npm-package-json-lint": "^6.3.0",
    "npm-package-json-lint-config-default": "^5.0.0",
    "sort-package-json": "^1.57.0",
    "typescript": "^4.5.2"
  }
}
