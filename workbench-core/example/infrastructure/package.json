{
  "name": "@amzn/workbench-core-infrastructure",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/awslabs/monorepo-for-service-workbench.git"
  },
  "homepage": "https://github.com/awslabs/monorepo-for-service-workbench",
  "bin": {
    "infrastructure": "lib/index.js"
  },
  "scripts": {
    "compile": "node scripts/buildLambda.js",
    "build": "rushx compile && heft build --clean",
    "make-badges": "istanbul-badges-readme --coverageDir=./temp/coverage --exitCode=1",
    "test": "rushx compile && heft test --clean --test-path-pattern infrastructure-stack && rushx make-badges",
    "test:only": "heft test -u --clean --test-path-pattern infrastructure-stack",
    "build:test": "rushx build && rushx test:only && rushx make-badges",
    "test:integration": "heft test -u --clean",
    "watch": "tsc -w",
    "cdk": "cdk",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:synth": "cdk synth",
    "cdk:deploy": "rushx compile && cdk deploy --all --require-approval never --outputs-file cdk-outputs.json"
  },
  "devDependencies": {
    "@types/node": "^14",
    "aws-cdk": "^2.12.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "@rushstack/heft": "^0.45.0",
    "@rushstack/heft-jest-plugin": "^0.2.3",
    "@rushstack/heft-node-rig": "^1.7.1",
    "@types/heft-jest": "1.0.2",
    "istanbul-badges-readme": "1.8.1",
    "@amzn/eslint-config-workbench-core-eslint-custom": "workspace:*",
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.22.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.22.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-authorizers-alpha": "^2.22.0-alpha.0",
    "eslint": "^8.7.0",
    "eslint-plugin-security": "^1.4.0",
    "@rushstack/eslint-config": "^2.5.1",
    "@types/jest": "^27.4.1",
    "@aws-cdk/assert": "^2.0.0",
    "jest": "^27.5.1",
    "express": "^4.17.3",
    "@types/express": "^4.17.13",
    "@vendia/serverless-express": "^4.5.4",
    "@types/aws-lambda": "^8.10.92",
    "axios": "^0.27.1",
    "cdk-nag": "^2.12.25",
    "esbuild": "^0.14.21",
    "@types/lodash": "^4.14.181"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.0.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.16",
    "@amzn/workbench-core-express": "workspace:*",
    "lodash": "^4.17.21"
  }
}
