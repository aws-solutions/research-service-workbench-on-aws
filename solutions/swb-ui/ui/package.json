{
  "name": "@aws/swb-ui",
  "version": "0.0.1",
  "description": "NextJS frontend UI for Service Workbench v2",
  "homepage": "https://github.com/aws-solutions/solution-spark-on-aws",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-solutions/solution-spark-on-aws.git"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
    "url": "http://aws.amazon.com"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "add-license-header": "license-check-and-add add -f license-add-config.json",
    "build": "next build && next export && rushx lint",
    "build:test": "rushx build && rushx test",
    "check-license-header": "license-check-and-add check -f license-add-config.json",
    "depcheck": "depcheck",
    "deploy-ui-and-api": "bash ./scripts/deploy.sh",
    "dev": "bash ./scripts/dev.sh",
    "e2e-test": "cypress run --env STAGE=${STAGE}",
    "e2e-test:open": "cypress open --env STAGE=${STAGE}",
    "license-checker": "license-checker --onlyAllow 'MIT; Apache-2.0; ISC; BSD'",
    "lint": "eslint . && rushx pkg-json-lint",
    "lint:fix": "eslint . --fix",
    "make-badges": "istanbul-badges-readme --coverageDir=./temp/coverage",
    "pkg-json-lint": "npmPkgJsonLint -c ../../../.npmpackagejsonlintrc.json .",
    "serve": "node .next/server/webpack-runtime.js",
    "sort-package-json": "sort-package-json package.json",
    "test": "jest --coverage --passWithNoTests 2>&1 && rushx make-badges",
    "test:only": "jest --ci --coverage --passWithNoTests 2>&1"
  },
  "dependencies": {
    "@aws/workbench-core-datasets-ui": "workspace:*",
    "@aws/workbench-core-environments": "workspace:*",
    "@aws/workbench-core-environments-ui": "workspace:*",
    "@aws/workbench-core-swb-common-ui": "workspace:*",
    "js-yaml": "^4.1.0",
    "next": "^12.1.5",
    "next-global-css": "^1.2.0",
    "next-transpile-modules": "^9.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@aws/eslint-config-workbench-core-eslint-custom": "workspace:*",
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@cloudscape-design/collection-hooks": "^1.0.3",
    "@cloudscape-design/components": "^3.0.27",
    "@cloudscape-design/design-tokens": "^3.0.1",
    "@cloudscape-design/global-styles": "^1.0.1",
    "@next/eslint-plugin-next": "^13.0.3",
    "@rushstack/eslint-config": "^3.0.0",
    "@rushstack/heft": "^0.47.5",
    "@rushstack/heft-jest-plugin": "^0.3.28",
    "@rushstack/heft-node-rig": "^1.10.5",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/heft-jest": "1.0.2",
    "@types/jest": "^27.4.1",
    "@types/node": "^14",
    "@types/react": "17.0.38",
    "babel-jest": "27.5.1",
    "cypress": "^10.6.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.7.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "5.0.1",
    "istanbul-badges-readme": "1.8.1",
    "jest": "^27.5.1",
    "license-check-and-add": "^4.0.5",
    "license-checker": "^25.0.1",
    "npm-package-json-lint": "^6.3.0",
    "npm-package-json-lint-config-default": "^5.0.0",
    "sass": "^1.49.8",
    "sharp": "^0.30.4",
    "sort-package-json": "^1.57.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.2"
  }
}
