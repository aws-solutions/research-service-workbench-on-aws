{
  "name": "@amzn/swb-ui",
  "version": "0.0.1",
  "description": "NextJS frontend UI for Service Workbench v2",
  "homepage": "https://github.com/awslabs/monorepo-for-service-workbench",
  "repository": {
    "type": "git",
    "url": "https://github.com/awslabs/monorepo-for-service-workbench.git"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
    "url": "http://aws.amazon.com"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "next build && rushx lint",
    "build:test": "rushx build && rushx test",
    "depcheck": "depcheck",
    "dev": "next dev",
    "export": "next build && next export",
    "lint": "next lint --max-warnings 0 && rushx pkg-json-lint",
    "lint:fix": "next lint --fix",
    "make-badges": "istanbul-badges-readme --coverageDir=./temp/coverage",
    "pkg-json-lint": "npmPkgJsonLint -c ../../.npmpackagejsonlintrc.json .",
    "serve": "node .next/server/webpack-runtime.js",
    "sort-package-json": "sort-package-json package.json",
    "start": "next start",
    "test": "jest --coverage --passWithNoTests 2>&1 && rushx make-badges",
    "test:only": "jest --ci --coverage --passWithNoTests 2>&1"
  },
  "dependencies": {
    "@amzn/environments": "workspace:*",
    "axios": "^0.27.1",
    "date-fns": "^2.28.0",
    "express": "^4.17.3",
    "next": "^12.1.5",
    "next-global-css": "^1.2.0",
    "next-transpile-modules": "^9.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@amzn/eslint-config-workbench-core-eslint-custom": "workspace:*",
    "@awsui/collection-hooks": "^1.0.0",
    "@awsui/components-react": "^3.0.0",
    "@awsui/design-tokens": "^3.0.0",
    "@awsui/global-styles": "^1.0.0",
    "@awsui/test-utils-core": "^1.0.0",
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@rushstack/eslint-config": "^2.5.1",
    "@rushstack/heft": "^0.45.0",
    "@rushstack/heft-jest-plugin": "^0.2.3",
    "@rushstack/heft-node-rig": "^1.7.1",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/express": "^4.17.13",
    "@types/heft-jest": "1.0.2",
    "@types/jest": "^27.4.1",
    "@types/node": "^14",
    "@types/react": "17.0.38",
    "babel-jest": "27.5.1",
    "concurrently": "^7.0.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.7.0",
    "eslint-config-next": "12.1.5",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-testing-library": "5.0.1",
    "istanbul-badges-readme": "1.8.1",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "npm-package-json-lint": "^6.3.0",
    "npm-package-json-lint-config-default": "^5.0.0",
    "sass": "^1.49.8",
    "sharp": "^0.30.4",
    "sort-package-json": "^1.57.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.2"
  }
}
