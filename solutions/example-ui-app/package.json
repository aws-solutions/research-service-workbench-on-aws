{
  "name": "@aws/example-ui-app",
  "version": "0.0.1",
  "description": "This is a prototype app and you should modify the source code to reflect your project needs",
  "homepage": "https://github.com/aws-solutions/research-service-workbench-on-aws",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-solutions/research-service-workbench-on-aws.git"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
    "url": "http://aws.amazon.com"
  },
  "scripts": {
    "add-license-header": "license-check-and-add add -f license-add-config.json",
    "prebuild": "next telemetry disable",
    "build": "rushx prebuild && next build && rushx lint",
    "build:test": "rushx build && rushx test",
    "check-license-header": "license-check-and-add check -f license-add-config.json",
    "depcheck": "depcheck",
    "dev": "next dev",
    "license-checker": "license-checker --onlyAllow 'MIT; Apache-2.0; ISC; BSD'",
    "lint": "next lint --max-warnings 0 && rushx pkg-json-lint",
    "lint:fix": "next lint --fix",
    "pkg-json-lint": "npmPkgJsonLint -c ../../.npmpackagejsonlintrc.json .",
    "serve": "node .next/server/webpack-runtime.js",
    "sort-package-json": "sort-package-json package.json",
    "start": "next start",
    "test": "jest --ci --coverage --passWithNoTests 2>&1"
  },
  "dependencies": {
    "next": "^12.1.5",
    "next-i18next": "^11.0.0",
    "next-transpile-modules": "^9.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@aws/eslint-config-workbench-core-eslint-custom": "workspace:*",
    "@awsui/components-react": "^3.0.0",
    "@awsui/design-tokens": "^3.0.0",
    "@awsui/global-styles": "^1.0.0",
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@next/eslint-plugin-next": "^13.0.3",
    "@rushstack/eslint-config": "^3.0.0",
    "@rushstack/heft": "^0.50.5",
    "@rushstack/heft-jest-plugin": "^0.5.11",
    "@testing-library/jest-dom": "5.16.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^18.15.11",
    "@types/react": "17.0.38",
    "babel-jest": "27.5.1",
    "depcheck": "^1.4.3",
    "eslint": "^8.7.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "5.0.1",
    "jest": "^27.5.1",
    "license-check-and-add": "^4.0.5",
    "license-checker": "^25.0.1",
    "npm-package-json-lint": "^6.3.0",
    "npm-package-json-lint-config-default": "^5.0.0",
    "sass": "^1.49.8",
    "sort-package-json": "^1.57.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.2"
  }
}
