    "@amzn/workbench-core-base": "workspace:*",
    "@aws-sdk/types": "^3.55.0"
  },
  "devDependencies": {
    "aws-cdk-lib": "2.19.0",
    "constructs": "^10.0.0",
    "cdk-ssm-document": "3.1.0",
    "cdk-iam-floyd": "0.300.0",
    "aws-lambda": "^1.0.7",
    "@rushstack/heft": "^0.44.2",
    "@rushstack/heft-node-rig": "^1.7.1",
    "@types/heft-jest": "1.0.2",
    "@types/node": "^12",
    "typescript": "^4.5.2",
    "eslint": "^8.7.0",
    "aws-cdk": "^2.12.0",
