{
  "extends": "@rushstack/heft-node-rig/profiles/default/config/jest.config.json",
  "collectCoverage": true,
  "transform": {
    "\\.(ts)$": "ts-jest"
  },
  "moduleFileExtensions": ["ts", "js"],
  "coveragePathIgnorePatterns": [
    "<rootDir>/src/accountRoutes.ts",
    "<rootDir>/src/apiRouteConfig.ts",
    "<rootDir>/src/authRoutes.ts",
    "<rootDir>/src/costCenterRoutes.ts",
    "<rootDir>/src/datasetRoutes.ts",
    "<rootDir>/src/environmentRoutes.ts",
    "<rootDir>/src/environmentTypeConfigRoutes.ts",
    "<rootDir>/src/environmentTypeRoutes.ts",
    "<rootDir>/src/generateRouter.ts",
    "<rootDir>/src/index.ts",
    "<rootDir>/src/projectRoutes.ts",
    "<rootDir>/src/staticPermissionsConfig.ts",
    "<rootDir>/src/staticRouteConfig.ts",
    "<rootDir>/src/userRoutes.ts",
    "<rootDir>/src/createProvisionDatasetRequest.ts",
    "<rootDir>/src/dataSet.ts",
    "<rootDir>/src/dataSetExternalEndpointRequest.ts",
    "<rootDir>/src/dataSetPlugin.ts",
    "<rootDir>/src/dataSetStoragePlugin.ts",
    "<rootDir>/src/endpointConnectionStrings.ts",
    "<rootDir>/src/externalEndpoint.ts",
    "<rootDir>/src/storageLocation.ts",

    "<rootDir>/src/validatorHelper.ts",
    "<rootDir>/src/errorHandlers.ts"

  ],
  "coverageThreshold": {
    "global": {
      "branches": 0,
      "functions": 70,
      "lines": 70,
      "statements": 70
    }
  },
  "coverageReporters": ["json-summary", "json"]
}
