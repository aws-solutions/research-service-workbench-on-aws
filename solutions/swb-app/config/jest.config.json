    "<rootDir>/src/keyPairs/sendPublicKeyRequest.ts",
    "<rootDir>/src/keyPairs/sendPublicKeyResponse.ts",

    "<rootDir>/src/validatorHelper.ts",
    "<rootDir>/src/errorHandlers.ts"
